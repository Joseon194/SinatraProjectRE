<h1>This is your car:</h1>

<h3>Name: <%= @car.name %> </h3>
<h3>Color: <%= @car.color %> </h3>
<h3>Model Year: <%= @car.model_year %> </h3>
<h3>Company: <%= @rock.company %> </h3>

<form action="/cars/<%= @car.id %>/edit" method="get">
    <input type="submit" value="Edit Vehicle">
</form>

<form action="/cars/<%= @car.id %>/delete" method="post">
    <input id="hidden" type="hidden" name="_method" value="delete">
    <input type="submit" value="Delete Vehicle">
</form>

<h3><a href="/home">Back to homepage.</a></h3>
